{
  "name": "@fundifyhub/logger",
  "version": "1.0.0",
  "description": "Simple logging utility for FundifyHub applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
